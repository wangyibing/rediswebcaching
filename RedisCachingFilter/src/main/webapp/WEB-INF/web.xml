<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <filter>
        <filter-name>RedisHTTPGetFilter</filter-name>
        <filter-class>com.ibm.zurich.rediscachingfilter.filters.RedisHTTPGetFilter</filter-class>
    </filter>
    <filter>
        <filter-name>MultiReadWriteFilter</filter-name>
        <filter-class>com.ibm.zurich.rediscachingfilter.filters.MultiReadWriteFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>MultiReadWriteFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>RedisHTTPGetFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>DummyServlet</servlet-name>
        <servlet-class>com.ibm.zurich.rediscachingfilter.servlets.DummyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>com.ibm.zurich.rediscachingfilter.servlets.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DummyServlet</servlet-name>
        <url-pattern>/DummyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/HelloServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
